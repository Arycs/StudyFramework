    self.Id = item[1];
    self.Desc = item[2];
    self.Name = item[3];
    self.RoleId = item[4];
    self.HeadPic = item[5];
    self.JobPic = item[6];
    self.JobDesc = item[7];
    self.Attack = item[8];
    self.Defense = item[9];
    self.Hit = item[10];
    self.Dodge = item[11];
    self.Cri = item[12];
    self.Res = item[13];

    return self;
end

