function DTJobEntity.New(Id, Desc, Name, RoleId, HeadPic, JobPic, JobDesc, Attack, Defense, Hit, Dodge, Cri, Res)
    local self = { };
    setmetatable(self, DTJobEntity);

    self.Id = Id;
    self.Desc = Desc;
    self.Name = Name;
    self.RoleId = RoleId;
    self.HeadPic = HeadPic;
    self.JobPic = JobPic;
    self.JobDesc = JobDesc;
    self.Attack = Attack;
    self.Defense = Defense;
    self.Hit = Hit;
    self.Dodge = Dodge;
    self.Cri = Cri;
    self.Res = Res;

    return self;
end

function DTJobEntity.NewFromArrItem(item)
    local self = { };
